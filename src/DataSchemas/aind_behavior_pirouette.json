          "title": "aind_behavior_services package version",
          "type": "string"
        },
        "version": {
          "const": "0.1.0-rc1",
          "default": "0.1.0-rc1",
          "title": "Version",
          "type": "string"
        },
        "computer_name": {
          "description": "Computer name",
          "title": "Computer Name",
          "type": "string"
        },
        "rig_name": {
          "description": "Rig name",
          "title": "Rig Name",
          "type": "string"
        },
        "camera_controller": {
          "$ref": "#/$defs/CameraController_SpinnakerCamera_",
          "description": "Required camera controller to triggered cameras."
        },
        "harp_white_rabbit": {
          "$ref": "#/$defs/HarpWhiteRabbit",
          "description": "Harp white rabbit"
        },
        "harp_output_expander": {
          "$ref": "#/$defs/HarpOutputExpander",
          "description": "Harp output expander"
        },
        "onix_commutator": {
          "$ref": "#/$defs/OnixCommutator",
          "description": "Onix commutator"
        },
        "robocopy_controller": {
          "$ref": "#/$defs/RobocopyController",
          "description": "Robocopy controller"
        }
      },
      "required": [
        "rig_name",
        "camera_controller",
        "harp_white_rabbit",
        "harp_output_expander",
        "onix_commutator",
        "robocopy_controller"
      ],
      "title": "AindBehaviorPirouetteRig",
      "type": "object"
    },
